{
  "version": 2,
  "framework": null,
  "builds": [
    {
      "src": "public/index.php",
      "use": "@vercel/php"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/public/index.php"
    }
    ],
    "env": {
        "VIEW_COMPILED_PATH": "/tmp/framework/views",
        "STORAGE_PATH": "/tmp/storage",
        "LOG_CHANNEL": "errorlog"
    }
  }
