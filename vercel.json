{
    "version": 2,
    "functions": {
      "api/**/*.php": {
        "runtime": "vercel-php@0.7.3",
        "memory": 1024,
        "maxDuration": 10
      }
    },
    "routes": [
      { "src": "/(.*)", "dest": "/api/index.php" }
    ],
    "env": {
        "VIEW_COMPILED_PATH": "/tmp/framework/views"
    }
  }
