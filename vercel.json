{
    "version": 2,
    "functions": {
      "api/index.php": {
        "runtime": "vercel-php@0.7.3"
      }
    },
    "builds": [
      { "src": "api/index.php",
        "use": "vercel-php@0.7.3" },
      { "src": "public/**/*",
        "use": "@vercel/static" }
    ],
    "routes": [
      { "src": "/(css|js|images)/(.*)",
        "dest": "/public/$1/$2" },
      { "src": "/(.*)",
        "dest": "/api/index.php" }
    ]
  }
